{
  "name": "wacm_group13",
  "version": "1.0.0",
  "scripts": {
    "start_local": "./start.sh",
    "stop_local": "./stop.sh",
    "start_environment": "docker-compose -f docker-compose-env.yml up",
    "stop_environment": "docker-compose -f docker-compose-env.yml down",
    "test": "npm run backend-test && npm run e2e-test",
    "backend-test": "./node_modules/.bin/jasmine-node backend/spec --captureExceptions",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e-test": "npm run e2e-setup && ./node_modules/.bin/nightwatch",
    "docker-test": "docker-compose -f docker-compose-test.yml up --build",
    "start": "npm run build && node server.js",
    "build": "npx webpack",
    "build-watch": "npx webpack --watch",
    "nodemon-server": "nodemon server.js",
    "nodemon-dev-server": "nodemon server.js -c ./config/dev_config.json",
    "dev-test-server": "node server.js -c ./config/test_config.json",
    "create-docs": "./node_modules/.bin/jsdoc -c ./docs/jsdoc.conf -t ./node_modules/ink-docstrap/template -d ./docs/html"
  },
  "private": true,
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsdoc-route-plugin": "^0.1.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "node-request-parser": "^1.0.3",
    "node-sass": "^4.7.2",
    "options-parser": "^0.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sanitizer": "^0.1.3",
    "sass-loader": "^6.0.6",
    "string-format": "^0.5.0",
    "striptags": "^3.1.1",
    "style-loader": "^0.19.1",
    "validator": "^9.4.1",
    "vue-localstorage": "^0.6.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "@types/node": "^9.3.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "jasmine-core": "^2.99.1",
    "jasmine-node": "^1.14.5",
    "jasmine-reporters": "^2.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "minimatch": "^3.0.4",
    "nightwatch": "^0.9.21",
    "request": "^2.83.0",
    "selenium-standalone": "^6.15.0",
    "vue": "^2.5.13",
    "vue-axios": "^2.0.2",
    "vue-loader": "^13.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  }
}
